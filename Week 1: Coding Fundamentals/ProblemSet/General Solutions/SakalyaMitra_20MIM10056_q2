#include<iostream>
using namespace std;

int main(){
    int x,y,z;
    cin>>x>>y>>z;
    if(z>x and z>y){
        cout<<"?"<<endl;
    }
    else{
        if(x>y){
            if(x>z){
                cout<<"+";
            }
            else{
                cout<<"?";
            }
        }
        else{
            if(y>z){
                cout<<"-";
            }
            else{
                cout<<"?";
            }
        }

    }
    return 0;
}
