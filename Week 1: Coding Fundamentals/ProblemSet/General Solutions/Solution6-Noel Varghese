#include<iostream>
using namespace std;
int main()
{
int PA=0,PB=0,n;
cout<<"Enter test cases"<<endl;
cin>>n;
cout<<"Enter coins in Pile A"<<endl;
cin>>PA;
cout<<"Enter coins in Pile B"<<endl;
cin>>PB;
if((PA!=0)&&(PB!=0))
{
PA=PA-2;
PB=PB-1;
if((PA==0)&&(PB==0))
{
cout<<"YES"<<endl;;
}
else
{
cout<<"NO"<<endl;
}
}
return 0;
}
